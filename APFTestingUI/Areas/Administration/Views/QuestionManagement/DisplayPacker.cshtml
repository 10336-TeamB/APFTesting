@model APFTestingUI.Areas.Administration.Models.QuestionManagement.Display

@{
    ViewBag.Title = "Packer Theory Question Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div id="manage-theory-question-wrap" class="details">
    <h1>Packer Question</h1>
    
    @Html.Partial("_DisplayQuestion", Model)

</div>

<br />
<br />

@Html.ActionLink("Back", "IndexPacker", new { }, new { @class="examiner-button" })

@if (Model.EditableOrDeletable)
{
    @Html.ActionLink("Edit", "EditPacker", new { questionId = Model.Id }, new { @class="examiner-button" })
    @Html.ActionLink("Delete", "DeletePacker", new { questionId = Model.Id }, new { @class="examiner-button delete" })
}

</div>

@section scripts
{
    <script src="~/Scripts/administration.js"></script>
}